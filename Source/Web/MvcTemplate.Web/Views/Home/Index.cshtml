@model MvcTemplate.Web.ViewModels.Home.IndexViewModel
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    @foreach (var joke in Model.Jokes)
    {
        <div class="col-md-4">
            @joke.Content
            <div>@joke.Category</div>
        </div>
    }
</div>
<div class="row">
    @foreach (var category in Model.Categories)
    {
        <div class="col-md-2">
            @category.Name
        </div>
    }
</div>